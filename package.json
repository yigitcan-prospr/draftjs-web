{
  "name": "draftjs-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "draft-js": "^0.11.0",
    "draft-js-export-html": "^1.3.3",
    "draft-js-import-html": "^1.3.3",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
